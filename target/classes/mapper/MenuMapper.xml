<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lds.tpms.dao.MenuMapper">

    <select id="selMenuByPid" resultType="com.lds.tpms.pojo.Menu">
        select * from `menu` where `pid` = #{pid} and `type` = #{empType}
    </select>

</mapper>